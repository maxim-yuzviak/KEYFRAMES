{
    "liveSassCompile.settings.generateMap": false,
    "liveSassCompile.settings.autoprefix": "last 5 versions",
  
  
    "liveSassCompile.settings.formats":[
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "/css"
        },
        {
            "extensionName": ".min.css",
            "format": "compressed",
            "savePath": "/css"
        }
    ],
    "liveSassCompile.settings.excludeList": [
    "/**/node_modules/**",
    "/.vscode/**"
    ]  
}
